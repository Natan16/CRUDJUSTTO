<%@ attribute name="id" required="true" %>

<input id="${id}" name="${id}" type="text" placeholder="dd/mm/aa" >
<script>
    $("#${id}").datepicker({dateFormat: 'dd/mm/yy' , changeYear : true ,  changeMonth: true});
    $("#${id}").datepicker.setDefaults( $.datepicker.regional[ "br" ] );
    
</script>
